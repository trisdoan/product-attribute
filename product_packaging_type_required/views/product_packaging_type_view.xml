<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_package_type_tree" model="ir.ui.view">
        <field name="name">stock.package.type.list.inherit</field>
        <field name="model">stock.package.type</field>
        <field name="priority" eval="8" />
        <field name="inherit_id" ref="stock.stock_package_type_tree" />
        <field name="arch" type="xml">
            <field name="barcode" position="after">
                <field name="required" />
            </field>
        </field>
    </record>
    <record id="view_stock_package_type_form" model="ir.ui.view">
        <field name="name">stock.package.type.form</field>
        <field name="model">stock.package.type</field>
        <field name="priority" eval="8" />
        <field name="inherit_id" ref="stock.stock_package_type_form" />
        <field name="arch" type="xml">
            <field name="barcode" position="after">
                <field name="required" />
            </field>
        </field>
    </record>
</odoo>
